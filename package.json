{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .eslintignore",
    "lint:strict": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0 --ignore-path .eslintignore",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --ignore-path .eslintignore",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:unit": "vitest run --dir src/__tests__/unit",
    "test:unit:watch": "vitest watch --dir src/__tests__/unit",
    "test:integration": "vitest run --dir src/__tests__/integration",
    "test:integration:watch": "vitest watch --dir src/__tests__/integration",
    "test:components": "vitest run --dir src/__tests__/components",
    "test:components:watch": "vitest watch --dir src/__tests__/components",
    "test:coverage": "vitest run --coverage",
    "analyze": "cross-env ANALYZE=1 vite build",
    "clean:dev": "node scripts/clean-dev-cache.js"
  },
  "overrides": {
    "clarinet": "0.6.0"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "camelcase-css": "^2.0.1",
    "combined-stream": "^1.0.8",
    "glob": "^11.0.3",
    "lucide-react": "^0.539.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-google-recaptcha": "^3.1.0",
    "react-router-dom": "^6.23.1",
    "react-router-hash-link": "^2.4.3",
    "source-map": "^0.7.6",
    "sourcemap-codec": "^1.4.8",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.28.0",
    "@types/estree": "^1.0.8",
    "@types/history": "^4.7.11",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.11.5",
    "@types/parse-json": "^4.0.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.9",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/testing-library__react": "^10.2.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "dom-accessibility-api": "^0.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.9.0",
    "jsdom": "^24.1.3",
    "lighthouse": "^12.8.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "sharp": "^0.34.3",
    "start-server-and-test": "^2.0.13",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vite-plugin-pwa": "^1.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:react/recommended"
    ],
    "rules": {
      "react/self-closing-comp": [
        "error",
        {
          "html": false
        }
      ]
    }
  }
}
