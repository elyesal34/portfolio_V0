{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:strict": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "preview": "vite preview",
    "test": "vitest --config vitest.config.ts",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --config vitest.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:integration": "vitest run --config vitest.config.ts src/__tests__/**/*.integration.test.tsx",
    "test:navigation": "playwright test tests/navigation.test.ts",
    "test:navigation:ui": "playwright test --ui tests/navigation.test.ts",
    "analyze": "cross-env ANALYZE=1 vite build",
    "perf:lighthouse": "start-server-and-test 'vite preview --port 4173' http://localhost:4173 \"lighthouse http://localhost:4173 --preset=desktop --only-categories=performance,accessibility,best-practices,seo --chrome-flags='--headless' --output html --output-path ./lighthouse-report.html\""
  },
  "overrides": {
    "clarinet": "0.6.0"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@testing-library/user-event": "^14.6.1",
    "glob": "^11.0.3",
    "lucide-react": "^0.539.0",
    "preact": "^10.27.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-google-recaptcha": "^3.1.0",
    "react-router-dom": "^7.7.1",
    "react-router-hash-link": "^2.4.3",
    "source-map": "^0.7.6",
    "sourcemap-codec": "^1.4.8",
    "vite-plugin-pwa": "^1.0.2",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0-rc.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.9",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.1",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^5.14.0",
    "sharp": "^0.34.3",
    "start-server-and-test": "^2.0.13",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:react/recommended"
    ],
    "rules": {
      "react/self-closing-comp": [
        "error",
        {
          "html": false
        }
      ]
    }
  }
}
